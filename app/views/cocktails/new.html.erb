<div class="container">
  <div class="row justify-content-center">
    <div class="col-12">
      <%= simple_form_for @cocktail do |c| %>
        <%= c.input :name %>
        <%= c.input :photo %>
        <%= c.input :photo_cache, as: :hidden %>
        <%= c.full_error :token %>
        <%= c.submit 'Save' %>
      <% end %>
    </div>
  </div>
</div>

